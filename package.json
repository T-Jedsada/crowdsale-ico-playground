{
  "name": "crowdsale-ico-playground",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.10.9",
    "dotenv": "^6.0.0",
    "moment": "^2.22.2",
    "openzeppelin-solidity": "^2.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.1",
    "reactflipclock-js": "^1.0.0",
    "reactide-config": "^1.0.2",
    "redux": "^4.0.1",
    "truffle-hdwallet-provider": "^0.0.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write \"src/**/*.{js,css}\"",
    "prettier-changefiles": "pretty-quick --staged",
    "precommit": "yarn run prettier-changefiles",
    "complie": "yarn lint && truffle complie",
    "deploy": "yarn lint && truffle migrate",
    "lint": "solhint \"contracts/**/*.sol\"",
    "start-ganache": "ganache-cli --networkId 1234567 --blockTime=0.1 -p 8545",
